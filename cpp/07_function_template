
//Шаблон функции
template <typename T>
T sum(T a, T b)
{
    return a + b;
}

int main()
{
    cout << sum(1, 2) << endl;
    cout << sum(1.0, 2.0) << endl;

    return 0;
}





//Шаблон функции с несколькими типами
template <typename T1, typename T2>
T1 sum(T1 a, T2 b)
{
    return a + b;
}

int main()
{
    cout << sum(1, 2.0) << endl;

    return 0;
}
